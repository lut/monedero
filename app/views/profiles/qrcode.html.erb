<br><br><br><br>

<div class="container">
  <div class="row">
    <div class="col-md-12">
    	<table>
            <% @qr.modules.each_index do |x| %>
            	<tr> 
    				<% @qr.modules.each_index do |y| %>
               			<% if @qr.dark?(x,y) %>
                			<td class="black"/>
              			<% else %>
                			<td class="white"/>
               		<% end %>

               		
              	<% end %>
              </tr>
            <% end %>
     	</table>
     </div>
   </div>
 </div>
