
<div class='page-header'>
     <h1>Todos los usuarios</h1>
</div>

<table class="table table-striped table-hover">
		<tr>
			<th>ID</th>
			<th>Nombre</th>
			<th>Mail</th>
		</tr>
		<% @user.each do |user| %>
	
		<tr>
			<td><%= user.id %></td>
			<td><%= user.full_name %> </td>
			<td><%= user.email %> </td>
		</tr>

	<% end %>
</table>



